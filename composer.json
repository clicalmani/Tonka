{
    "name": "clicalmani/tonka",
    "type": "project",
    "description": "Tonka Framework.",
    "keywords": ["framework", "tonka"],
    "license": "MIT",
    "require": {
        "chillerlan/php-qrcode": "^5.0",
        "clicalmani/console": ">=1.0",
        "clicalmani/database": ">=1.0",
        "clicalmani/foundation": ">=2.0.3",
        "clicalmani/routing": ">=1.0",
        "clicalmani/xpower": ">=1.2.1",
        "dompdf/dompdf": "^2.0",
        "fedapay/fedapay-php": "^0.4.0",
        "guzzlehttp/guzzle": "^7.9",
        "kwn/number-to-words": "^2.6",
        "mpdf/mpdf": "^8.1",
        "php-amqplib/php-amqplib": "^3.6",
        "phpoffice/phpspreadsheet": "^1.23",
        "phpoffice/phpword": "^1.0",
        "phpstan/phpstan": "^1.10",
        "symfony/console": "^6.3",
        "webklex/php-imap": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Database\\Entities\\": "database/entities/",
            "Test\\": "test/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
